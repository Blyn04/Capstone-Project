{
  "expo": {
    "name": "NULS MOA",
    "slug": "NULS-MOA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./components/images/NULS_Favicon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./components/images/NULS_Favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nuls.numoa",
      "deploymentTarget": "15.1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification",
          "background-fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./components/images/NULS_Favicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nuls.numoa",
      "googleServicesFile": "./android/app/google-services.json",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./components/images/NULS_Favicon.png"
    },
    "owner": "blynsu",
    "extra": {
      "eas": {
        "projectId": "ab29db21-d917-4e43-89ae-1c8e875dea34"
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-notifications",
        {
          "icon": "./components/images/NULS_Favicon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "notification": {
      "icon": "./components/images/NULS_Favicon.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} new interactions"
    }
  }
}
